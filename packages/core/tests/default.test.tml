// Tests for core::default module
use test
use core::default

@test
func test_i32_default() -> I32 {
    let x: I32 = I32::default()
    assert(x == 0, "I32::default() should be 0")
    return 0
}

@test
func test_i64_default() -> I32 {
    let x: I64 = I64::default()
    assert(x == 0, "I64::default() should be 0")
    return 0
}

@test
func test_bool_default() -> I32 {
    let x: Bool = Bool::default()
    assert(x == false, "Bool::default() should be false")
    return 0
}

@test
func test_f64_default() -> I32 {
    let x: F64 = F64::default()
    assert(x == 0.0, "F64::default() should be 0.0")
    return 0
}

@test
func test_str_default() -> I32 {
    let x: Str = Str::default()
    assert(x == "", "Str::default() should be empty string")
    return 0
}
