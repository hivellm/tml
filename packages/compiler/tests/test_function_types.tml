// Test function types

// Type aliases for function types
type UnaryOp = func(I32) -> I32
type BinaryOp = func(I32, I32) -> I32
type Predicate = func(I32) -> Bool
type Action = func()

// Functions that match these types
func add_one(x: I32) -> I32 {
    return x + 1
}

func add(a: I32, b: I32) -> I32 {
    return a + b
}

func is_positive(x: I32) -> Bool {
    return x > 0
}

func say_hello() {
    println(42)
}

func main() {
    println(add_one(5))
    println(add(3, 4))
}
