// Test IO functions from runtime/io.c
use test

@test
func test_print_string() -> I32 {
    print("hello")
    print(" ")
    print("world")
    return 0
}

@test
func test_println_string() -> I32 {
    println("line 1")
    println("line 2")
    return 0
}

@test
func test_print_i32() -> I32 {
    print(42)
    print(" ")
    print(-123)
    println("")
    return 0
}

@test
func test_print_i64() -> I32 {
    let big: I64 = 9223372036854775807
    print(big)
    println("")
    return 0
}

// F64 test skipped due to codegen issue with float types

@test
func test_print_bool() -> I32 {
    print(true)
    print(" ")
    print(false)
    println("")
    return 0
}

