// Test closure syntax

type UnaryOp = func(I32) -> I32
type BinaryOp = func(I32, I32) -> I32

func main() {
    // Simple closure with explicit type
    let add_one: UnaryOp = do(x: I32) -> I32 x + 1

    // Closure with block body
    let add_two: UnaryOp = do(x: I32) -> I32 {
        return x + 2
    }

    // Closure with multiple parameters
    let add: BinaryOp = do(a: I32, b: I32) -> I32 a + b

    // Test calling closures
    let result1: I32 = add_one(5)
    println(result1)

    let result2: I32 = add_two(10)
    println(result2)

    let result3: I32 = add(3, 4)
    println(result3)
}
