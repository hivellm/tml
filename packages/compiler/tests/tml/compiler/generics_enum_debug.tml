// Debug generic enum
type Maybe[T] {
    Just(T),
    Nothing,
}

func main() -> I32 {
    print("Creating Just(42)")
    let m: Maybe[I32] = Just(42)
    print("Done creating, starting when")
    when m {
        Just(v) => {
            print("Matched Just")
            print(v)
            return 0
        },
        Nothing => {
            print("Matched Nothing")
            return 1
        },
    }
    print("No match")
    return 2
}
