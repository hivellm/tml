// Test 11: More expressions
func main() {
    // Compound expressions
    let a: I32 = 2 + 3 * 4
    println(a)  // 14

    let b: I32 = (2 + 3) * 4
    println(b)  // 20

    // Negative numbers
    let c: I32 = -5
    println(c)  // -5

    let d: I32 = 10 + c
    println(d)  // 5

    // Modulo
    let e: I32 = 17 % 5
    println(e)  // 2

    // Boolean expressions
    let x: I32 = 5
    println(x > 3)  // true
    println(x < 3)  // false

    // Storing comparison results
    let gt3: I32 = x > 3
    let lt10: I32 = x < 10
    println(gt3)  // true
    println(lt10)  // true

    // Complex boolean with stored values
    let both: Bool = gt3 and lt10
    println(both)  // true

    // Boolean literals
    let p: Bool = true
    let q: Bool = false
    println(p or q)  // true
    println(p and q)  // false
    println(not q)  // true
}
