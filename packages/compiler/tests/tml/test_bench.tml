func bench_int_add(iterations: I32) -> I32 {
    let mut sum: I32 = 0
    let mut i: I32 = 0
    loop {
        if i >= iterations {
            break
        }
        sum = sum + i
        i = i + 1
    }
    return sum
}

func main() -> I32 {
    println("Testing bench_int_add")
    let start: I32 = time_ms()
    let r: I32 = bench_int_add(100000)
    let t: I32 = time_ms() - start
    println("Time: {} ms, result = {}", t, r)
    return 0
}
