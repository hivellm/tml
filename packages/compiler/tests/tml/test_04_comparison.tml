// Test 04: Comparison and logical operators
func main() {
    let a: I32 = 10
    let b: I32 = 20

    // Comparison operators
    println(a == b)   // false
    println(a != b)   // true
    println(a < b)    // true
    println(a > b)    // false
    println(a <= b)   // true
    println(a >= b)   // false

    // Logical operators (TML uses words)
    let x: Bool = true
    let y: Bool = false

    println(x and y)  // false
    println(x or y)   // true
    println(not x)    // false
}
