type Point {
    x: I32,
    y: I32,
}

type Rectangle {
    origin: Point,
    width: I32,
    height: I32,
}

func main() {
    let p: Point = Point { x: 10, y: 20 }
    println(p.x)
    println(p.y)
    let rect: Rectangle = Rectangle { origin: Point { x: 0, y: 0 }, width: 100, height: 50 }
    println(rect.width)
    println(rect.height)
    println(rect.origin.x)
}
