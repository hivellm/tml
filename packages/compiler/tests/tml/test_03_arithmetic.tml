// Test 03: Arithmetic - binary and unary operations
func main() {
    // Basic arithmetic
    let a: I32 = 10
    let b: I32 = 3

    println(a + b)  // 13
    println(a - b)  // 7
    println(a * b)  // 30
    println(a / b)  // 3
    println(a % b)  // 1

    // Unary minus
    let neg: I32 = -a
    println(neg)  // -10

    // Compound expressions
    let result: I32 = (a + b) * 2 - 1
    println(result)  // 25
}
