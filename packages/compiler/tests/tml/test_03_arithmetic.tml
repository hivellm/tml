// Test 03: Arithmetic - binary and unary operations
func main() {
    // Basic arithmetic
    let a = 10
    let b = 3

    println(a + b)  // 13
    println(a - b)  // 7
    println(a * b)  // 30
    println(a / b)  // 3
    println(a % b)  // 1

    // Unary minus
    let neg = -a
    println(neg)  // -10

    // Compound expressions
    let result = (a + b) * 2 - 1
    println(result)  // 25
}
