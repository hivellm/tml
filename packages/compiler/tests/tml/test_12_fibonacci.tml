// Test 12: Fibonacci (recursive and iterative)
func fib_recursive(n: I32) -> I32 {
    if n <= 1 {
        return n
    }
    return fib_recursive(n - 1) + fib_recursive(n - 2)
}

func fib_iterative(n: I32) -> I32 {
    if n <= 1 {
        return n
    }

    let a: I32 = 0
    let b: I32 = 1
    let i: I32 = 2

    loop {
        if i > n {
            break
        }
        let temp: I32 = a + b
        a = b
        b = temp
        i = i + 1
    }

    return b
}

func main() {
    // Test recursive fibonacci
    println(fib_recursive(0))  // 0
    println(fib_recursive(1))  // 1
    println(fib_recursive(5))  // 5
    println(fib_recursive(10)) // 55

    // Test iterative fibonacci
    println(fib_iterative(0))  // 0
    println(fib_iterative(1))  // 1
    println(fib_iterative(5))  // 5
    println(fib_iterative(10)) // 55
}
