// Floor returns the greatest integer value less than or equal to x

// For F64, we need to implement proper floor logic
pub func floor(x: F64) -> F64 {
    // Convert to I64 truncates toward zero
    let i: I64 = f64_to_i64(x)
    let f: F64 = i64_to_f64(i)

    // If x is negative and not exactly an integer, subtract 1
    return if x < f then f - 1.0 else f
}

// Integer versions are identity functions
pub func floor_i32(n: I32) -> I32 {
    return n
}

pub func floor_i64(n: I64) -> I64 {
    return n
}
