use test

// Basic I/O Functions Tests
// print() is polymorphic - accepts Str, I32, Bool, F64, etc.

@test
func test_print_basic() -> I32 {
    print("Hello from test")
    return 0
}

@test
func test_println_basic() -> I32 {
    println("Hello with newline")
    return 0
}

@test
func test_print_i32_positive() -> I32 {
    print(42)
    return 0
}

@test
func test_print_i32_negative() -> I32 {
    print(-100)
    return 0
}

@test
func test_print_i32_zero() -> I32 {
    print(0)
    return 0
}

@test
func test_print_bool_true() -> I32 {
    print(true)
    return 0
}

@test
func test_print_bool_false() -> I32 {
    print(false)
    return 0
}

@test
func test_multiple_prints() -> I32 {
    print("Value: ")
    print(123)
    println("")
    return 0
}
