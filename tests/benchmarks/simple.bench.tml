// Simple benchmark test

@bench
func bench_addition() {
    let _x: I32 = 1 + 2 + 3 + 4 + 5
}

@bench(10000)  // Run 10,000 iterations
func bench_loop() {
    let mut sum: I32 = 0
    let mut i: I32 = 0
    loop {
        if i >= 100 {
            break
        }
        sum = sum + i
        i = i + 1
    }
}

@bench(5000)  // Run 5,000 iterations
func bench_multiplication() {
    let _x: I64 = 123456 * 789012
}
