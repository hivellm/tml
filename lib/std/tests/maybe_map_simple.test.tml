// Simple test for Maybe map combinator
// Tests the builtin Maybe type with map-like operations
use test

@test
func test_maybe_just() {
    let x: Maybe[I32] = Just(5)
    assert_eq(x.unwrap_or(0), 5)
    println("Maybe Just test passed!")
}

@test
func test_maybe_nothing() {
    let n: Maybe[I32] = Nothing
    assert_eq(n.unwrap_or(42), 42)
    println("Maybe Nothing test passed!")
}
