// Tests for std::text â€” uncovered methods: includes, push_log, push_path, print, println
use test::{assert, assert_eq}
use std::text::Text

@test
func test_text_includes() -> I32 {
    let t = Text::from("hello world")
    assert(t.includes("world"), "includes should find 'world'")
    assert(not t.includes("xyz"), "includes should not find 'xyz'")
    0
}

@test
func test_text_push_log() -> I32 {
    let t = Text::new()
    t.push_log("key=", 42, " val=", 100, " total=", 142, ".")
    let s = t.as_str()
    assert(s.len() > 0, "push_log should produce non-empty text")
    0
}

@test
func test_text_push_path() -> I32 {
    let t = Text::new()
    t.push_path("dir/", 1, "/file", 2, ".txt")
    let s = t.as_str()
    assert(s.len() > 0, "push_path should produce non-empty text")
    0
}

@test
func test_text_print() -> I32 {
    let t = Text::from("test output")
    t.print()
    // Just verify it doesn't crash
    0
}

@test
func test_text_println() -> I32 {
    let t = Text::from("test line")
    t.println()
    // Just verify it doesn't crash
    0
}
