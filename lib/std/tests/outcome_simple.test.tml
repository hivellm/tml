// Simple Outcome[T, E] combinator test
// Tests the builtin Outcome type with unwrap and is_* operations
use test

@test
func test_outcome_ok_unwrap() {
    let x: Outcome[I32, I32] = Ok(5)
    assert_eq(x.unwrap_or(0), 5)
    println("Outcome Ok unwrap test passed!")
}

@test
func test_outcome_err_unwrap() {
    let a: Outcome[I32, I32] = Err(42)
    assert_eq(a.unwrap_or(0), 0)
    println("Outcome Err unwrap test passed!")
}

@test
func test_outcome_is_ok() {
    let x: Outcome[I32, I32] = Ok(10)
    assert(x.is_ok(), "Ok(10).is_ok() should be true")
    println("Outcome is_ok test passed!")
}

@test
func test_outcome_is_err() {
    let a: Outcome[I32, I32] = Err(99)
    assert(a.is_err(), "Err(99).is_err() should be true")
    println("Outcome is_err test passed!")
}
