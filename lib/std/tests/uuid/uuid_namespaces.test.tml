use test
use std::uuid::Uuid

@test
func test_namespace_dns() -> I32 {
    let ns = Uuid::namespace_dns()
    assert_eq(ns.to_string(), "6ba7b810-9dad-11d1-80b4-00c04fd430c8")
    assert_eq(ns.version(), 1)
    assert_eq(ns.variant(), 2)
    0
}

