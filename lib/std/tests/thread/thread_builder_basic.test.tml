@test
func test_builder_new() -> I32 {
    let b: Builder = Builder::new()
    // Builder::new should not crash
    assert(true, "Builder::new works")
    return 0
}

@test
func test_builder_name() -> I32 {
    let b: Builder = Builder::new().name("worker-1")
