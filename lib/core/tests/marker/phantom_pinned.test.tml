// Tests for core::marker â€” PhantomPinned
use test::{assert, assert_eq}
use core::marker::PhantomPinned

@test
func test_phantom_pinned_new() -> I32 {
    let pp: PhantomPinned = PhantomPinned::new()
    let s: Str = pp.to_string()
    assert_eq(s, "PhantomPinned", "to_string should be PhantomPinned")
    return 0
}

@test
func test_phantom_pinned_default() -> I32 {
    let pp: PhantomPinned = PhantomPinned::default()
    let dbg: Str = pp.debug_string()
    assert_eq(dbg, "PhantomPinned", "debug_string should be PhantomPinned")
    return 0
}

@test
func test_phantom_pinned_duplicate() -> I32 {
    let pp: PhantomPinned = PhantomPinned::new()
    let pp2: PhantomPinned = pp.duplicate()
    assert_eq(pp2.to_string(), "PhantomPinned", "duplicate should work")
    return 0
}
