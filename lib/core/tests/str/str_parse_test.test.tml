}

@test
func test_str_parse_bool_true() -> I32 {
    let s: Str = "true"
    let result: Maybe[Bool] = str::parse_bool(s)
    when result {
        Just(v) => assert(v, "parse_bool true"),
        Nothing => assert(false, "parse_bool should succeed")
    }
    return 0
}
