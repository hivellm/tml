// Tests for Str methods â€” slice_str and as_bytes (uncovered)
use test::{assert, assert_eq}

@test
func test_str_slice_str_basic() -> I32 {
    let s: Str = "hello world"
    let sub: Str = s.slice_str(0, 5)
    assert_eq(sub, "hello", "slice_str 0..5 should be hello")
    return 0
}

@test
func test_str_slice_str_middle() -> I32 {
    let s: Str = "hello world"
    let sub: Str = s.slice_str(6, 11)
    assert_eq(sub, "world", "slice_str 6..11 should be world")
    return 0
}

@test
func test_str_slice_str_empty() -> I32 {
    let s: Str = "hello"
    let sub: Str = s.slice_str(2, 2)
    assert_eq(sub, "", "slice_str 2..2 should be empty")
    return 0
}
