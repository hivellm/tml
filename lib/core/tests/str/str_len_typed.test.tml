// Test: Str.len() with typed variable binding
use test

@test
func test_str_len_typed() -> I32 {
    let s: Str = "hello"
    let n: I64 = s.len()
    assert_eq(n, 5, "len typed")
    return 0
}

@test
func test_str_len_comparison() -> I32 {
    let s: Str = "hello"
    assert(s.len() > 0, "len > 0")
    return 0
}
