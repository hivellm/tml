use test
use core::str

@test
func test_str_pad_left() -> I32 {
    let s: Str = "hello"
    let result: Str = str::pad_left(s, 10, " ")
    assert_eq(result, "     hello", "pad_left")
    return 0
}

@test
func test_str_pad_right() -> I32 {
    let s: Str = "hello"
    let result: Str = str::pad_right(s, 10, " ")
    assert_eq(result, "hello     ", "pad_right")
    return 0
}
