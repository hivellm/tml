// Tests for core::cache - constants
use test
use core::cache::{CACHE_LINE_SIZE, HALF_CACHE_LINE, DOUBLE_CACHE_LINE}

@test
func test_cache_line_size() -> I32 {
    assert_eq(CACHE_LINE_SIZE, 64 as I64, "cache line should be 64 bytes")
    return 0
}

@test
func test_half_cache_line() -> I32 {
    assert_eq(HALF_CACHE_LINE, 32 as I64, "half cache line should be 32 bytes")
    return 0
}

@test
func test_double_cache_line() -> I32 {
    assert_eq(DOUBLE_CACHE_LINE, 128 as I64, "double cache line should be 128 bytes")
    return 0
}
