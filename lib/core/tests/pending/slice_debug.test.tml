// Minimal slice test for debugging

use test

use core::slice::{Slice}

@test
func test_slice_len_debug() -> I32 {
    let ptr: *Unit = alloc(20)

    // Create slice from raw pointer
    let slice: Slice[I32] = Slice {
        data: ptr,
        len: 5
    }

    let len_val: I64 = slice.len()

    // Debug: print what we got
    assert(len_val == 5, "len should be 5")

    dealloc(ptr)
    return 0
}
