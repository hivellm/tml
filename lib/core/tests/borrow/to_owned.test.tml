// Tests for core::borrow::ToOwned â€” primitive type to_owned
use test::{assert, assert_eq}
use core::borrow::ToOwned

@test
func test_to_owned_i32() -> I32 {
    let x: I32 = 42
    let y: I32 = x.to_owned()
    assert_eq(y, 42, "I32 to_owned should preserve value")
    return 0
}

@test
func test_to_owned_i64() -> I32 {
    let x: I64 = 99999
    let y: I64 = x.to_owned()
    assert_eq(y as I32, 99999 as I32, "I64 to_owned should preserve value")
    return 0
}

@test
func test_to_owned_i8() -> I32 {
    let x: I8 = -10 as I8
    let y: I8 = x.to_owned()
    assert_eq(y as I32, -10, "I8 to_owned should preserve value")
    return 0
}

@test
func test_to_owned_i16() -> I32 {
    let x: I16 = 1000 as I16
    let y: I16 = x.to_owned()
    assert_eq(y as I32, 1000, "I16 to_owned should preserve value")
    return 0
}

@test
func test_to_owned_u8() -> I32 {
    let x: U8 = 200 as U8
    let y: U8 = x.to_owned()
    assert_eq(y as I32, 200, "U8 to_owned should preserve value")
