@test
func test_to_owned_bool() -> I32 {
    let x: Bool = true
    let y: Bool = x.to_owned()
    assert(y, "Bool to_owned should preserve true")
    let z: Bool = false
    let w: Bool = z.to_owned()
    assert(not w, "Bool to_owned should preserve false")
    return 0
}

@test
func test_to_owned_str() -> I32 {
    let x: Str = "hello"
    let y: Str = x.to_owned()
    assert_eq(y, "hello", "Str to_owned should preserve value")
    return 0
}

@test
func test_to_owned_f32() -> I32 {
    let x: F32 = 1.5 as F32
    let y: F32 = x.to_owned()
    assert(y > 1.4 as F32, "F32 to_owned should preserve value")
    assert(y < 1.6 as F32, "F32 to_owned should preserve value")
    return 0
}

@test
func test_to_owned_f64() -> I32 {
    let x: F64 = 3.14
    let y: F64 = x.to_owned()
    assert(y > 3.13, "F64 to_owned should preserve value")
    assert(y < 3.15, "F64 to_owned should preserve value")
    return 0
}
