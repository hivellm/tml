// Tests for UnsafeCell::default and SyncUnsafeCell::default
use test::{assert_eq}
use core::cell::unsafe_cell::{UnsafeCell, SyncUnsafeCell}

@test
func test_unsafe_cell_default() -> I32 {
    let cell: UnsafeCell[I32] = UnsafeCell::default()
    let val: I32 = cell.into_inner()
    assert_eq(val, 0, "UnsafeCell default should contain I32 default (0)")
    return 0
}

@test
func test_sync_unsafe_cell_default() -> I32 {
    let cell: SyncUnsafeCell[I32] = SyncUnsafeCell::default()
    let val: I32 = cell.into_inner()
    assert_eq(val, 0, "SyncUnsafeCell default should contain I32 default (0)")
    return 0
}
