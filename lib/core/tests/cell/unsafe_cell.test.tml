// Tests for core::cell::unsafe_cell â€” UnsafeCell, SyncUnsafeCell
use test::{assert, assert_eq}
use core::cell::unsafe_cell::{UnsafeCell, SyncUnsafeCell}

@test
func test_unsafe_cell_new_and_into_inner() -> I32 {
    let cell: UnsafeCell[I64] = UnsafeCell::new(42 as I64)
    let val: I64 = cell.into_inner()
    assert_eq(val, 42 as I64, "UnsafeCell should store and return value")
    return 0
}

@test
func test_sync_unsafe_cell_new_and_into_inner() -> I32 {
    let cell: SyncUnsafeCell[I32] = SyncUnsafeCell::new(99)
    let val: I32 = cell.into_inner()
    assert_eq(val, 99, "SyncUnsafeCell should store and return value")
    return 0
}
