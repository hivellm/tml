// Tests for primitive .eq() method - coverage for I8::eq through Bool::eq
use test

@test
func test_i8_eq() -> I32 {
    let a: I8 = 10
    let b: I8 = 10
    assert(a.eq(ref b), "I8: 10.eq(10)")
    let c: I8 = -5
    assert(not a.eq(ref c), "I8: 10.eq(-5)")
    return 0
}

