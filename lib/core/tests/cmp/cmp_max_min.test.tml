// Tests for cmp::max and cmp::min free functions
use test::{assert, assert_eq}
use core::cmp::{max, min}

@test
func test_max_i32() -> I32 {
    let result: I32 = max(3, 7)
    assert_eq(result, 7, "max(3, 7) should be 7")
    return 0
}

@test
func test_min_i32() -> I32 {
    let result: I32 = min(3, 7)
    assert_eq(result, 3, "min(3, 7) should be 3")
    return 0
}

@test
func test_max_equal() -> I32 {
    let result: I32 = max(5, 5)
    assert_eq(result, 5, "max(5, 5) should be 5")
    return 0
}
