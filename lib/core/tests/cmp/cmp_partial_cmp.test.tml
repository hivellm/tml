// Tests for partial_cmp on various integer types
use test
use core::cmp::{Ordering, PartialOrd}

// =============================================================================
// I32 partial_cmp (already tested elsewhere, but adding for coverage)
// =============================================================================

@test
func test_i32_partial_cmp_less() -> I32 {
    let a: I32 = 1
    let b: I32 = 5
    let result: Maybe[Ordering] = a.partial_cmp(ref b)
    when result {
        Just(ord) => assert(ord.is_less(), "1.partial_cmp(5) should be Less"),
        Nothing => assert(false, "I32 partial_cmp should not return Nothing"),
    }
    return 0
}

@test
func test_i32_partial_cmp_equal() -> I32 {
    let a: I32 = 7
    let b: I32 = 7
    let result: Maybe[Ordering] = a.partial_cmp(ref b)
    when result {
        Just(ord) => assert(ord.is_equal(), "7.partial_cmp(7) should be Equal"),
        Nothing => assert(false, "I32 partial_cmp should not return Nothing"),
    }
    return 0
}

@test
func test_i32_partial_cmp_greater() -> I32 {
    let a: I32 = 10
    let b: I32 = 3
    let result: Maybe[Ordering] = a.partial_cmp(ref b)
    when result {
        Just(ord) => assert(ord.is_greater(), "10.partial_cmp(3) should be Greater"),
        Nothing => assert(false, "I32 partial_cmp should not return Nothing"),
    }
    return 0
}

