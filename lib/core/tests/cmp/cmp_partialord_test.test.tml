// Tests for PartialOrd default methods: lt, le, gt, ge
// And Ord::partial_cmp default, Maybe::eq, Outcome::eq
use test
use core::cmp::{Ordering, PartialOrd}

// =============================================================================
// PartialOrd default methods: .lt(), .le(), .gt(), .ge()
// =============================================================================

@test
func test_i32_lt() -> I32 {
    let a: I32 = 5
    let b: I32 = 10
    assert(a.lt(ref b), "5.lt(10) should be true")
    assert(not b.lt(ref a), "10.lt(5) should be false")
    assert(not a.lt(ref a), "5.lt(5) should be false")
    return 0
}

@test
func test_i32_le() -> I32 {
    let a: I32 = 5
    let b: I32 = 10
    assert(a.le(ref b), "5.le(10) should be true")
    let c: I32 = 5
    assert(a.le(ref c), "5.le(5) should be true")
    assert(not b.le(ref a), "10.le(5) should be false")
    return 0
}

@test
func test_i32_gt() -> I32 {
    let a: I32 = 10
    let b: I32 = 5
    assert(a.gt(ref b), "10.gt(5) should be true")
    assert(not b.gt(ref a), "5.gt(10) should be false")
    assert(not a.gt(ref a), "10.gt(10) should be false")
    return 0
}

@test
func test_i32_ge() -> I32 {
    let a: I32 = 10
    let b: I32 = 5
    assert(a.ge(ref b), "10.ge(5) should be true")
    let c: I32 = 10
    assert(a.ge(ref c), "10.ge(10) should be true")
    assert(not b.ge(ref a), "5.ge(10) should be false")
    return 0
}
