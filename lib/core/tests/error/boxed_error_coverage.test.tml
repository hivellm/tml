// BoxedError::new[E: Error] blocked by trait constraint codegen (Unknown method: to_string)
use test::{assert, assert_eq}
use core::error::BoxedError

@test
func test_boxed_error_message_field() -> I32 {
    let b: BoxedError = BoxedError::from_message("test error")
    assert_eq(b.message, "test error", "BoxedError message field accessible")
    return 0
}
