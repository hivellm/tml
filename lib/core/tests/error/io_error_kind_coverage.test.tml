// Tests for IoErrorKind to_string and debug_string â€” uncovered variants
use test::{assert, assert_eq}
use core::error::{IoError, IoErrorKind}

// === to_string coverage for untested variants ===

@test
func test_io_kind_connection_reset_to_string() -> I32 {
    let err = IoError::new(IoErrorKind::ConnectionReset)
    assert_eq(err.to_string(), "connection reset", "ConnectionReset message")
    return 0
}

