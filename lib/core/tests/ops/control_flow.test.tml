// Tests for core::ops::try_trait â€” ControlFlow
use test::{assert, assert_eq}
use core::ops::try_trait::ControlFlow

@test
func test_control_flow_continue_is_continue() -> I32 {
    let cf: ControlFlow[I32, Str] = ControlFlow::Continue(42)
    assert(cf.is_continue(), "Continue should be continue")
    assert(not cf.is_break(), "Continue should not be break")
    return 0
}

@test
func test_control_flow_break_is_break() -> I32 {
    let cf: ControlFlow[I32, Str] = ControlFlow::Break("error")
    assert(cf.is_break(), "Break should be break")
    assert(not cf.is_continue(), "Break should not be continue")
    return 0
}

@test
func test_control_flow_continue_i64() -> I32 {
    let cf: ControlFlow[I64, I32] = ControlFlow::Continue(100 as I64)
    assert(cf.is_continue(), "Continue I64 should be continue")
    return 0
}

@test
func test_control_flow_break_i32() -> I32 {
    let cf: ControlFlow[I64, I32] = ControlFlow::Break(1)
    assert(cf.is_break(), "Break I32 should be break")
    return 0
}
