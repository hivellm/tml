// Tests for arithmetic behavior methods on U32
use test

@test
func test_u32_add_method() -> I32 {
    let a: U32 = 100
    let b: U32 = 200
    let r: U32 = a.add(b)
    assert_eq(r as I32, 300, "U32.add()")
    return 0
}

@test
func test_u32_sub_method() -> I32 {
    let a: U32 = 500
    let b: U32 = 200
    let r: U32 = a.sub(b)
    assert_eq(r as I32, 300, "U32.sub()")
    return 0
}

@test
func test_u32_mul_method() -> I32 {
    let a: U32 = 15
    let b: U32 = 20
    let r: U32 = a.mul(b)
    assert_eq(r as I32, 300, "U32.mul()")
