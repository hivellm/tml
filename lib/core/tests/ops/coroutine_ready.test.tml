// Tests for Ready[T] and FromIter[I] coroutine helpers
use test::{assert}
use core::ops::coroutine::{Ready, FromIter}

@test
func test_ready_new() -> I32 {
    let r: Ready[I32] = Ready::new(42)
    assert(true, "Ready::new should construct without crash")
    return 0
}

@test
func test_ready_exhausted() -> I32 {
    let r: Ready[I32] = Ready::exhausted()
    assert(true, "Ready::exhausted should construct without crash")
    return 0
}

@test
func test_from_iter_new() -> I32 {
    let fi: FromIter[I32] = FromIter::new(0)
    assert(true, "FromIter::new should construct without crash")
    return 0
}
