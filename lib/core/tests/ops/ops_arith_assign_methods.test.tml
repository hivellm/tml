// Tests for ops::arith assign trait methods via explicit .add_assign() etc.
// Covers all types: I8, I16, I32, I64, U8, U16, U32, U64, F32, F64
use test

// =============================================================================
// I8 assign methods
// =============================================================================

@test
func test_i8_add_assign() -> I32 {
    var a: I8 = 10
    a.add_assign(5)
    assert_eq(a as I32, 15, "I8.add_assign()")
    return 0
}

@test
func test_i8_sub_assign() -> I32 {
    var a: I8 = 20
    a.sub_assign(5)
    assert_eq(a as I32, 15, "I8.sub_assign()")
    return 0
}

@test
func test_i8_mul_assign() -> I32 {
    var a: I8 = 6
    a.mul_assign(7)
    assert_eq(a as I32, 42, "I8.mul_assign()")
    return 0
}

@test
func test_i8_div_assign() -> I32 {
    var a: I8 = 100
    a.div_assign(5)
    assert_eq(a as I32, 20, "I8.div_assign()")
    return 0
}

@test
func test_i8_rem_assign() -> I32 {
    var a: I8 = 17
    a.rem_assign(5)
    assert_eq(a as I32, 2, "I8.rem_assign()")
    return 0
}

