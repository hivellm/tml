// Tests for core::char::UNICODE_VERSION tuple constant
use test::{assert_eq}
use core::char::UNICODE_VERSION

@test
func test_unicode_version_major() -> I32 {
    let (major, minor, patch) = UNICODE_VERSION
    assert_eq(major, 15 as U8, "major version should be 15")
    return 0
}

@test
func test_unicode_version_minor() -> I32 {
    let (major, minor, patch) = UNICODE_VERSION
    assert_eq(minor, 1 as U8, "minor version should be 1")
    return 0
}

@test
func test_unicode_version_patch() -> I32 {
    let (major, minor, patch) = UNICODE_VERSION
    assert_eq(patch, 0 as U8, "patch version should be 0")
    return 0
}
