// Tests for core::num::nonzero - NonZero[I32]
use test::{assert, assert_eq}
use core::num::nonzero::NonZero

@test
func test_nonzero_new_valid() -> I32 {
    let result: Maybe[NonZero[I32]] = NonZero::new(42)
    assert(result.is_just(), "NonZero::new(42) should be Just")
    return 0
}

@test
func test_nonzero_new_zero() -> I32 {
    let result: Maybe[NonZero[I32]] = NonZero::new(0)
    assert(result.is_nothing(), "NonZero::new(0) should be Nothing")
    return 0
}

@test
func test_nonzero_get() -> I32 {
    let result: Maybe[NonZero[I32]] = NonZero::new(42)
    let nz: NonZero[I32] = result.unwrap()
    assert_eq(nz.get(), 42, "get() returns inner value")
    return 0
}

@test
func test_nonzero_negative() -> I32 {
    let result: Maybe[NonZero[I32]] = NonZero::new(-5)
    assert(result.is_just(), "NonZero::new(-5) should be Just")
    let nz: NonZero[I32] = result.unwrap()
    assert_eq(nz.get(), -5, "get() returns negative value")
    return 0
}
