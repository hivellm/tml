// Tests for core::convert module - minimal tests

use test
use core::convert::{From}

// Just test I32 identity to avoid codegen issues
@test
func test_from_identity_i32() -> I32 {
    let x: I32 = 42
    let y: I32 = I32::from(x)
    assert_eq(y, 42, "identity conversion should preserve value")
    return 0
}

