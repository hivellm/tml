// Single test for From conversion
use test
use core::convert::{From}

func main() -> I32 {
    // Test I8 to I16
    let x: I8 = 42
    let y: I16 = I16::from(x)
    print("I8 to I16: ")
    print((y as I32).to_string())
    print("\n")

    // Test negative
    let a: I8 = -42
    let b: I16 = I16::from(a)
    print("I8 -42 to I16: ")
    print((b as I32).to_string())
    print("\n")

    // Test U8 to U16
    let u1: U8 = 255
    let u2: U16 = U16::from(u1)
    print("U8 255 to U16: ")
    print((u2 as I32).to_string())
    print("\n")

    // Test Bool to I32
    let bt: Bool = true
    let bi: I32 = I32::from(bt)
    print("Bool true to I32: ")
    print(bi.to_string())
    print("\n")

    return 0
}
