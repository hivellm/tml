// Tests for core::convert â€” TryFrom unsigned narrowing conversions
use test::{assert, assert_eq}
use core::convert::{TryFrom, TryFromIntError}

@test
func test_tryfrom_u64_to_u32_ok() -> I32 {
    let result: Outcome[U32, TryFromIntError] = U32::try_from(999 as U64)
    assert(result.is_ok(), "999 fits in U32")
    return 0
}

@test
func test_tryfrom_u64_to_u32_overflow() -> I32 {
    let result: Outcome[U32, TryFromIntError] = U32::try_from(5000000000 as U64)
    assert(result.is_err(), "5 billion overflows U32")
    return 0
}

@test
func test_tryfrom_u64_to_u8_ok() -> I32 {
    let result: Outcome[U8, TryFromIntError] = U8::try_from(200 as U64)
    assert(result.is_ok(), "200 fits in U8")
    return 0
}
