// Tests for core::convert â€” From[Bool] for I32/I64, From F32->F64
use test::{assert, assert_eq}
use core::convert::From

@test
func test_from_bool_true_to_i32() -> I32 {
    let v: I32 = I32::from(true)
    assert_eq(v, 1, "true to I32 should be 1")
    return 0
}

@test
func test_from_bool_false_to_i32() -> I32 {
    let v: I32 = I32::from(false)
    assert_eq(v, 0, "false to I32 should be 0")
    return 0
}

@test
func test_from_bool_true_to_i64() -> I32 {
    let v: I64 = I64::from(true)
    assert_eq(v, 1, "true to I64 should be 1")
    return 0
}

@test
func test_from_bool_false_to_i64() -> I32 {
    let v: I64 = I64::from(false)
    assert_eq(v, 0, "false to I64 should be 0")
    return 0
}

@test
func test_from_f32_to_f64() -> I32 {
    let v: F64 = F64::from(3.14 as F32)
    assert(v > 3.13 and v < 3.15, "F32 3.14 to F64 should be near 3.14")
    return 0
}
