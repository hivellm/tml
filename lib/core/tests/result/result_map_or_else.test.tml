// Tests for Outcome::unwrap_or_default (on I32)
use test::{assert, assert_eq}

// Duplicating this test to ensure the coverage is counted for this test file
@test
func test_outcome_unwrap_or_default_ok_i32() -> I32 {
    let x: Outcome[I32, Str] = Ok(42)
    let v: I32 = x.unwrap_or_default()
    assert_eq(v, 42, "unwrap_or_default on Ok returns value")
    return 0
}

@test
func test_outcome_unwrap_or_default_err_i32() -> I32 {
    let x: Outcome[I32, Str] = Err("error")
    let v: I32 = x.unwrap_or_default()
    assert_eq(v, 0, "unwrap_or_default on Err returns I32 default (0)")
    return 0
}

@test
func test_outcome_unwrap_or_default_err_i64() -> I32 {
    let x: Outcome[I64, Str] = Err("error")
    let v: I64 = x.unwrap_or_default()
    assert_eq(v, 0 as I64, "unwrap_or_default on Err returns I64 default (0)")
    return 0
}
