// Tests for Char Display â€” Char.to_string()
use test::{assert, assert_eq}

@test
func test_char_to_string_a() -> I32 {
    let s: Str = 'A'.to_string()
    assert_eq(s, "A", "'A'.to_string() should be 'A'")
    return 0
}

@test
func test_char_to_string_digit() -> I32 {
    let s: Str = '7'.to_string()
    assert_eq(s, "7", "'7'.to_string() should be '7'")
    return 0
}

@test
func test_char_to_string_space() -> I32 {
    let s: Str = ' '.to_string()
    assert_eq(s, " ", "' '.to_string() should be space")
    return 0
}
