// Tests targeting uncovered fmt/impls debug_string methods
use test
use core::fmt::impls

@test
func test_i8_debug_string() -> I32 {
    let a: I8 = 42 as I8
    let s: Str = a.debug_string()
    assert(s.len() > 0, "I8 debug_string")
    return 0
}

