// Tests for Ordering to_string and debug_string via fmt/impls
use test

// =============================================================================
// Ordering::to_string
// =============================================================================

@test
func test_ordering_less_to_string() -> I32 {
    let o: Ordering = Ordering::Less
    let s: Str = o.to_string()
    assert(s.len() > 0, "Ordering Less to_string")
    return 0
}

@test
func test_ordering_equal_to_string() -> I32 {
    let o: Ordering = Ordering::Equal
    let s: Str = o.to_string()
    assert(s.len() > 0, "Ordering Equal to_string")
    return 0
}

@test
func test_ordering_greater_to_string() -> I32 {
    let o: Ordering = Ordering::Greater
    let s: Str = o.to_string()
    assert(s.len() > 0, "Ordering Greater to_string")
    return 0
}

