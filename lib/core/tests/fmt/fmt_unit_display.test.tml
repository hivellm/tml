use test::{assert, assert_eq}
use core::fmt::Alignment

@test
func test_alignment_left_to_string() -> I32 {
    let a: Alignment = Alignment::Left
    let s: Str = a.to_string()
    assert_eq(s, "Left", "Alignment::Left to_string")
    return 0
}

