// Tests for core::fmt::FmtError
use test::{assert, assert_eq}
use core::fmt::FmtError

@test
func test_fmt_error_new() -> I32 {
    let e: FmtError = FmtError::new()
    let s: Str = e.to_string()
    assert_eq(s, "formatting error", "FmtError to_string")
    return 0
}

@test
func test_fmt_error_debug() -> I32 {
    let e: FmtError = FmtError::new()
    let dbg: Str = e.debug_string()
    assert_eq(dbg, "FmtError", "FmtError debug_string")
    return 0
}
