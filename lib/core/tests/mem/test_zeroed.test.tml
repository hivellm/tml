use test
use core::mem

@test
func test_mem_zeroed_i32() -> I32 {
    let x: I32 = mem::zeroed[I32]()
    assert(x == 0, "zeroed I32 should be 0")
    return 0
}

@test
func test_mem_zeroed_i64() -> I32 {
    let x: I64 = mem::zeroed[I64]()
    assert(x == 0 as I64, "zeroed I64 should be 0")
    return 0
}

@test
func test_mem_zeroed_bool() -> I32 {
    let x: Bool = mem::zeroed[Bool]()
    assert(x == false, "zeroed Bool should be false")
    return 0
}
