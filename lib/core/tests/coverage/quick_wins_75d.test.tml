// Math intrinsics coverage tests
use test::{assert_eq, assert}
use core::intrinsics::{sin, cos, floor, ceil, trunc, exp, fabs}

@test
func test_floor_f64() -> I32 {
    let x: F64 = 3.7
    let result = floor[F64](x)
    assert_eq(result, 3.0, "floor(3.7) should be 3.0")
    return 0
}

@test
func test_cos_f64() -> I32 {
    let x: F64 = 0.0
    let result = cos[F64](x)
    assert_eq(result, 1.0, "cos(0.0) should be 1.0")
    return 0
}

@test
func test_sin_f64() -> I32 {
    let x: F64 = 0.0
    let result = sin[F64](x)
    assert_eq(result, 0.0, "sin(0.0) should be 0.0")
    return 0
}

@test
func test_ceil_f64() -> I32 {
    let x: F64 = 3.2
    let result = ceil[F64](x)
    assert_eq(result, 4.0, "ceil(3.2) should be 4.0")
    return 0
}

@test
func test_trunc_f64() -> I32 {
    let x: F64 = 3.9
    let result = trunc[F64](x)
    assert_eq(result, 3.0, "trunc(3.9) should be 3.0")
    return 0
}

@test
func test_exp_f64() -> I32 {
    let x: F64 = 0.0
    let result = exp[F64](x)
    assert_eq(result, 1.0, "exp(0.0) should be 1.0")
    return 0
}

@test
func test_fabs_f64() -> I32 {
    let x: F64 = -5.0
    let result = fabs[F64](x)
    assert_eq(result, 5.0, "fabs(-5.0) should be 5.0")
    return 0
}
