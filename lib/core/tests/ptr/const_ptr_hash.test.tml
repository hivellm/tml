// Tests for core::ptr::const_ptr â€” RawPtr hash
use test::{assert, assert_eq}
use core::ptr::const_ptr::RawPtr

@test
func test_raw_ptr_hash() -> I32 {
    let ptr: RawPtr[I64] = RawPtr::from_addr(42 as I64)
    let h: I64 = ptr.hash()
    assert_eq(h, 42 as I64, "hash should return address")
    return 0
}

@test
func test_raw_ptr_hash_null() -> I32 {
    let ptr: RawPtr[I32] = RawPtr::null_ptr()
    let h: I64 = ptr.hash()
    assert_eq(h, 0 as I64, "null ptr hash should be 0")
    return 0
}

@test
func test_raw_ptr_to_usize() -> I32 {
    let ptr: RawPtr[I64] = RawPtr::from_addr(8888 as I64)
    assert_eq(ptr.to_usize(), 8888 as I64, "to_usize should return address")
    return 0
}
