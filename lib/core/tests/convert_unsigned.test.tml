// Tests for core::convert - From behavior (unsigned widening)
use test::{assert, assert_eq}
use core::convert::From

@test
func test_from_u8_to_u16() -> I32 {
    let val: U16 = U16::from(255 as U8)
    assert_eq(val as I64, 255 as I64, "U8 -> U16")
    return 0
}

@test
func test_from_u8_to_u32() -> I32 {
    let val: U32 = U32::from(200 as U8)
    assert_eq(val as I64, 200 as I64, "U8 -> U32")
    return 0
}

@test
func test_from_u8_to_u64() -> I32 {
    let val: U64 = U64::from(128 as U8)
    assert_eq(val as I64, 128 as I64, "U8 -> U64")
    return 0
}

@test
func test_from_u16_to_u32() -> I32 {
    let val: U32 = U32::from(65535 as U16)
    assert_eq(val as I64, 65535 as I64, "U16 -> U32")
    return 0
}

@test
func test_from_u16_to_u64() -> I32 {
    let val: U64 = U64::from(65535 as U16)
    assert_eq(val as I64, 65535 as I64, "U16 -> U64")
    return 0
}

@test
func test_from_u32_to_u64() -> I32 {
    let val: U64 = U64::from(4000000 as U32)
    assert_eq(val as I64, 4000000 as I64, "U32 -> U64")
    return 0
}
