@test
func test_duplicate_f32() -> I32 {
    let x: F32 = 3.14 as F32
    let y: F32 = x.duplicate()
    assert(y > 3.13 as F32, "F32 duplicate should preserve value")
    assert(y < 3.15 as F32, "F32 duplicate should preserve value")
    return 0
}

@test
func test_duplicate_f64() -> I32 {
    let x: F64 = 2.71828
    let y: F64 = x.duplicate()
    assert(y > 2.718, "F64 duplicate should preserve value")
    assert(y < 2.719, "F64 duplicate should preserve value")
    return 0
}

@test
func test_duplicate_bool_true() -> I32 {
    let x: Bool = true
    let y: Bool = x.duplicate()
    assert(y, "Bool true duplicate should be true")
    return 0
}

@test
func test_duplicate_bool_false() -> I32 {
    let x: Bool = false
    let y: Bool = x.duplicate()
    assert(not y, "Bool false duplicate should be false")
    return 0
}
