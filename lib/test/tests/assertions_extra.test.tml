use test::{assert, assert_eq, assert_false, assert_true, assert_str_empty, assert_str_not_empty}

@test
func test_assert_false_passes() -> I32 {
    assert_false(false, "false should pass assert_false")
    return 0
}

@test
func test_assert_true_passes() -> I32 {
    assert_true(true, "true should pass assert_true")
    return 0
}

@test
func test_assert_str_empty_passes() -> I32 {
    assert_str_empty("", "empty string should pass")
    return 0
}

@test
func test_assert_str_not_empty_passes() -> I32 {
    assert_str_not_empty("hello", "non-empty string should pass")
    return 0
}
