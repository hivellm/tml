{
  "name": "tree-sitter-tml",
  "version": "0.1.0",
  "description": "Tree-sitter grammar for TML (To Machine Language)",
  "main": "bindings/node",
  "keywords": [
    "tree-sitter",
    "tml",
    "parser",
    "syntax-highlighting"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/tml-lang/tree-sitter-tml"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "nan": "^2.17.0"
  },
  "devDependencies": {
    "tree-sitter-cli": "^0.20.8"
  },
  "scripts": {
    "build": "tree-sitter generate",
    "test": "tree-sitter test",
    "parse": "tree-sitter parse"
  },
  "tree-sitter": [
    {
      "scope": "source.tml",
      "file-types": [
        "tml"
      ],
      "highlights": "queries/highlights.scm",
      "injections": "queries/injections.scm",
      "locals": "queries/locals.scm"
    }
  ]
}
