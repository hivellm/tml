// TML Compiler Resource File
// Defines icon and version information for Windows executables

#include <windows.h>

// Icon resource
IDI_ICON1 ICON "../../icon.ico"

// Version information
VS_VERSION_INFO VERSIONINFO
FILEVERSION 0,1,6,0
PRODUCTVERSION 0,1,6,0
FILEFLAGSMASK 0x3F
FILEFLAGS 0x0
FILEOS VOS__WINDOWS32
FILETYPE VFT_APP
FILESUBTYPE VFT2_UNKNOWN
BEGIN
    BLOCK "StringFileInfo"
    BEGIN
        BLOCK "040904E4"
        BEGIN
            VALUE "CompanyName", "TML Project"
            VALUE "FileDescription", "TML Compiler"
            VALUE "FileVersion", "0.1.6.0"
            VALUE "InternalName", "tml.exe"
            VALUE "LegalCopyright", "Copyright (c) 2026"
            VALUE "OriginalFilename", "tml.exe"
            VALUE "ProductName", "TML"
            VALUE "ProductVersion", "0.1.6.0"
        END
    END
    BLOCK "VarFileInfo"
    BEGIN
        VALUE "Translation", 0x409, 1252
    END
END
