// Debug test for List[Buffer]
use test
use std::collections::{Buffer, List}

@test
func test_list_buffer_create() -> I32 {
    // Just create an empty list
    var samples: List[Buffer] = List[Buffer].default()
    samples.clear()
    return 0
}

@test
func test_list_buffer_push() -> I32 {
    // Create list and push one item
    var samples: List[Buffer] = List[Buffer].default()
    var sample1: Buffer = Buffer::from_string("Test")
    samples.push(sample1)
    samples.clear()
    return 0
}
