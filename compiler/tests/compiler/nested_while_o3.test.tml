use test

// Minimal nested loop test for O3 issue

@test
func test_nested_loops() -> I32 {
    var total: I32 = 0
    var i: I32 = 0
    loop (i < 3) {
        var j: I32 = 0
        loop (j < 3) {
            total = total + 1
            j = j + 1
        }
        i = i + 1
    }
    assert_eq(total, 9, "3x3 nested loop should count to 9")
    return 0
}
